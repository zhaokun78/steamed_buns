// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"label": "创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		},
		"name": {
			"bsonType": "string",
			"description": "参数名称",
			"label": "参数名称"
		},
		"value": {
			"bsonType": "string",
			"description": "参数值",
			"label": "参数值"
		},
		"memo": {
			"bsonType": "string",
			"description": "参数说明",
			"label": "参数说明"
		},
		"image": {
			"bsonType": "file",
			"title": "图片",
			"description": "图片",
			"fileMediaType": "image",
			"fileExtName": "jpg,png"
		}
	}
}
